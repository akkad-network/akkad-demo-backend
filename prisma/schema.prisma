datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model CrossChainDepositEvent {
  id              Int      @id @default(autoincrement())
  contractAddress String
  userAddressFrom String
  userAddressTo   String
  amount          String
  tokenAddress    String
  status          String
  createAt        DateTime @default(now())
}
